{
  "$ref": "#/definitions/PhotosSetupResponse",
  "$schema": "http://json-schema.org/draft-07/schema#",
  "definitions": {
    "PhotosSetupResponse": {
      "description": "The expected response format for the photos setup request",
      "properties": {
        "data": {
          "properties": {
            "moreComing": {
              "const": false,
              "description": "Should always be false",
              "type": "boolean"
            },
            "syncToken": {
              "description": "Sync token - currently not used",
              "minLength": 1,
              "type": "string"
            },
            "zones": {
              "description": "The list of photos account zones - either primary or primary and shared",
              "items": {
                "description": "Response zone object from photos setup response",
                "properties": {
                  "zoneID": {
                    "properties": {
                      "ownerRecordName": {
                        "minLength": 1,
                        "type": "string"
                      },
                      "zoneName": {
                        "minLength": 1,
                        "pattern": "^PrimarySync|SharedSync-[0-9A-F-]+$",
                        "type": "string"
                      },
                      "zoneType": {
                        "const": "REGULAR_CUSTOM_ZONE",
                        "type": "string"
                      }
                    },
                    "required": [
                      "zoneName",
                      "ownerRecordName",
                      "zoneType"
                    ],
                    "type": "object"
                  }
                },
                "required": [
                  "zoneID"
                ],
                "type": "object"
              },
              "maxItems": 2,
              "minItems": 1,
              "type": "array"
            }
          },
          "required": [
            "moreComing",
            "syncToken",
            "zones"
          ],
          "type": "object"
        }
      },
      "required": [
        "data"
      ],
      "type": "object"
    }
  }
}
